<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home Page</title>
  </head>
  <body>
    <h2>Recently Added</h2>
    <div>
      <% books.forEach(book => { %>
      <a href="/books/<%= book.id %>">
        <img height="150" width="100" src="<%= book.coverImagePath %>"/>
      </a>
      <% }) %>
    </div>
  </body>
</html>
